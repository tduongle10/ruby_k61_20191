<% provide(:title, "Home")%>
<% if logged_in? %>
<div class="row">
    <aside class="col-md-6">
        <section class="user_info">
            <%= render 'shared/user_info' %>
        </section>
        <section class="stats">
        <%= render 'shared/stats' %>
        </section>
    </aside>
    <section class="micropost_form col-md-6">
        <%= render 'shared/micropost_form' %>
    </section>
    <section class="feed">
        <%= render 'shared/feed' %>
    </section>
</div>
<% end %>